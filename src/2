#include "Dictionary.hpp"

using namespace PDF::Objects;
using namespace std;


void
Dictionary::print(ostream &out) const
{
	out << "<<";

	for (auto it = items.begin(); it != items.end(); it++) {
		(*it)->print(out);
	}

	out << ">>";
}


void
Dictionary::addItem(string key, shared_ptr<Object> value)
{
	items.emplace(key, value);
}
